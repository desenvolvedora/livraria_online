{
    "name": "panique/php-mvc",
    "type": "project",
    "description": "A simple PHP MVC boilerplate",
    "keywords": ["mvc", "skeleton", "boilerplate", "naked", "barebone", "application"],
    "homepage": "https://github.com/panique/php-mvc",
    "license": "MIT",
    "authors": [
        {
            "name": "Panique",
            "homepage": "http://careers.stackoverflow.com/panique",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/panique/php-mvc/issues",
        "source": "https://github.com/panique/php-mvc"
    },
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"
